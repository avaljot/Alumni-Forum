<h1>Threads created by You:</h1>
{{# each user.posts}}
    <li>
        <form method="post" action="/posts/getPost">
            <input type="hidden" value={{_id}} id='postId' name="getPost"/>
            <input type="submit" value="{{title}}" class="btn btn-default">
        </form>
    </li>
{{/each}}